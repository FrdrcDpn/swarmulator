<?xml version="1.0"?>
<parameters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="parameters.xsd">
  <!-- Simulation -->
  <Kpphi>1</Kpphi>
  <Kiphi>0.1</Kiphi>
  <Kdphi>0.3</Kdphi>
  <Kptheta>1</Kptheta>
  <Kitheta>0.1</Kitheta>
  <Kdtheta>0.3</Kdtheta>
  <Kpx>5</Kpx>
  <Kix>0</Kix>
  <Kdx>3</Kdx>
  <Kpy>5</Kpy>
  <Kiy>0</Kiy>
  <Kdy>3</Kdy>
  <Kpz>2</Kpz>
  <Kiz>0.01</Kiz>
  <Kdz>1</Kdz>
  <enable_UWB>0</enable_UWB>
  <dynamic_beacons>0</dynamic_beacons>
  <EKF_frequency>1000</EKF_frequency> 
  <IMU_frequency>1000</IMU_frequency> 
  <trajectory_frequency>100</trajectory_frequency> 
  <UWB_frequency>80</UWB_frequency> 
  <UWB_signal_length>0.01</UWB_signal_length>
  <max_UWB_range>4</max_UWB_range>
  <beacon_1_en>1</beacon_1_en>
  <beacon_2_en>0</beacon_2_en>
  <beacon_3_en>1</beacon_3_en>
  <beacon_4_en>0</beacon_4_en>
  <beacon_5_en>1</beacon_5_en>
  <beacon_6_en>1</beacon_6_en>
  <beacon_7_en>1</beacon_7_en>
  <beacon_8_en>1</beacon_8_en>
  <beacon_1_freq>100</beacon_1_freq>
  <beacon_2_freq>100</beacon_2_freq>
  <beacon_3_freq>100</beacon_3_freq>
  <beacon_4_freq>100</beacon_4_freq>
  <beacon_5_freq>100</beacon_5_freq>
  <beacon_6_freq>100</beacon_6_freq>
  <beacon_7_freq>100</beacon_7_freq>
  <beacon_8_freq>100</beacon_8_freq>
  <beacon_dynamic_freq>100</beacon_dynamic_freq>
  <dynamic_cov_approach>1</dynamic_cov_approach>
  <x_beacon_1>5</x_beacon_1>
  <y_beacon_1>5</y_beacon_1>
  <x_beacon_2>5</x_beacon_2>
  <y_beacon_2>-5</y_beacon_2>
  <x_beacon_3>-5</x_beacon_3>
  <y_beacon_3>-5</y_beacon_3>
  <x_beacon_4>-5</x_beacon_4>
  <y_beacon_4>5</y_beacon_4>
  <x_beacon_5>0</x_beacon_5>
  <y_beacon_5>5</y_beacon_5>
  <x_beacon_6>5</x_beacon_6>
  <y_beacon_6>0</y_beacon_6>
  <x_beacon_7>0</x_beacon_7>
  <y_beacon_7>-5</y_beacon_7>
  <x_beacon_8>-5</x_beacon_8>
  <y_beacon_8>0</y_beacon_8>
  <acc_noise_sigma>0.01</acc_noise_sigma>
  <noise_motor_sigma>0.005</noise_motor_sigma>
  <noise_type>3</noise_type>
  <gauss_sigma>0.025</gauss_sigma>
  <htc_gamma>0.1</htc_gamma>
  <htc_ratio>1.0</htc_ratio>
  <htg_mu>0.3</htg_mu>
  <htg_k>2.0</htg_k>
  <htg_lambda>3.5</htg_lambda>
  <htg_scale>1.0</htg_scale>
  <terminaloutput>0.0</terminaloutput>
  <simulation_updatefreq>1000</simulation_updatefreq>
  <simulation_realtimefactor>1</simulation_realtimefactor>
  <time_limit>0</time_limit>
  <!-- (seconds). Use time_limit > 0 to set a limit. If = 0, then the simulation runs indefinitely. -->
  <environment>square</environment>
  <!-- See the folder conf/environments/ for available environments. 
       Create your own or use "random" to generate a random map at runtime -->
  <!-- Animation -->
  <window_width>800</window_width>
  <window_height>800</window_height>
  <scale>0.15</scale>
  <mouse_drag_speed>0.3</mouse_drag_speed>
  <mouse_zoom_speed>0.4</mouse_zoom_speed>
  <zoom>-15</zoom>
  <!-- Start up zoom, bounded -90 and 9 -->
  <animation_updatefreq>200</animation_updatefreq>
  <!-- Logger -->
  <logger_updatefreq>2</logger_updatefreq>
  <!-- Additional parameters -->
  <policy/>
  <fitness>aggregation_clusters</fitness>
  <!--new_parameter_name>default value</new_parameter_name-->
</parameters>
