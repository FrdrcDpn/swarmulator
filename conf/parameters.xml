<?xml version="1.0"?>
<parameters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="parameters.xsd">
  <!-- Simulation -->
  <dynamic_beacons>1</dynamic_beacons>
  <UWB_interval>0.04</UWB_interval> 
  <beacon_1_en>1</beacon_1_en>
  <beacon_2_en>1</beacon_2_en>
  <beacon_3_en>1</beacon_3_en>
  <beacon_4_en>0</beacon_4_en>
  <beacon_5_en>0</beacon_5_en>
  <beacon_6_en>0</beacon_6_en>
  <beacon_7_en>0</beacon_7_en>
  <beacon_8_en>0</beacon_8_en>
  <x_beacon_1>20</x_beacon_1>
  <y_beacon_1>20</y_beacon_1>
  <x_beacon_2>20</x_beacon_2>
  <y_beacon_2>-20</y_beacon_2>
  <x_beacon_3>-20</x_beacon_3>
  <y_beacon_3>-20</y_beacon_3>
  <x_beacon_4>-20</x_beacon_4>
  <y_beacon_4>20</y_beacon_4>
  <x_beacon_5>0</x_beacon_5>
  <y_beacon_5>20</y_beacon_5>
  <x_beacon_6>20</x_beacon_6>
  <y_beacon_6>0</y_beacon_6>
  <x_beacon_7>0</x_beacon_7>
  <y_beacon_7>-20</y_beacon_7>
  <x_beacon_8>-20</x_beacon_8>
  <y_beacon_8>0</y_beacon_8>
  <noise_type>3</noise_type>
  <gauss_sigma>0.1</gauss_sigma>
  <htc_gamma>0.1</htc_gamma>
  <htc_ratio>1.0</htc_ratio>
  <htg_mu>0.1</htg_mu>
  <htg_k>2.0</htg_k>
  <htg_lambda>3.5</htg_lambda>
  <htg_scale>1.0</htg_scale>
  <simulation_updatefreq>20</simulation_updatefreq>
  <simulation_realtimefactor>300</simulation_realtimefactor>
  <time_limit>300</time_limit>
  <!-- (seconds). Use time_limit > 0 to set a limit. If = 0, then the simulation runs indefinitely. -->
  <environment>square</environment>
  <!-- See the folder conf/environments/ for available environments. 
       Create your own or use "random" to generate a random map at runtime -->
  <!-- Animation -->
  <window_width>800</window_width>
  <window_height>800</window_height>
  <scale>0.2</scale>
  <mouse_drag_speed>0.3</mouse_drag_speed>
  <mouse_zoom_speed>0.4</mouse_zoom_speed>
  <zoom>-40</zoom>
  <!-- Start up zoom, bounded -90 and 9 -->
  <animation_updatefreq>25</animation_updatefreq>
  <!-- Logger -->
  <logger_updatefreq>2</logger_updatefreq>
  <!-- Additional parameters -->
  <policy></policy>
  <fitness>aggregation_clusters</fitness>
  <!--new_parameter_name>default value</new_parameter_name-->
</parameters>
